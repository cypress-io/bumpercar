{
  "name": "@cypress/bumpercar",
  "version": "0.0.0-development",
  "description": "Easily update settings and trigger builds across projects and CI Providers.",
  "main": "index.js",
  "scripts": {
    "release": "releaser",
    "test": "NODE_ENV=test mocha",
    "watch": "NODE_ENV=test mocha --watch",
    "semantic-release": "semantic-release",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "issues": "git-issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cypress-io/cypress-bumpercar.git"
  },
  "keywords": [
    "deployment",
    "releasing",
    "versioning"
  ],
  "files": [
    "lib"
  ],
  "author": "Loren Norman <loren@cypress.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cypress-io/cypress-bumpercar/issues"
  },
  "homepage": "https://github.com/cypress-io/cypress-bumpercar#readme",
  "dependencies": {
    "@cypress/coffee-script": "0.1.2",
    "axios": "0.19.0",
    "bluebird": "3.7.1",
    "check-more-types": "^2.24.0",
    "debug": "^3.1.0",
    "lazy-ass": "^1.6.0",
    "lodash": "^4.17.4",
    "parse-github-repo-url": "^1.4.1",
    "ramda": "^0.26.0"
  },
  "devDependencies": {
    "@cypress/releaser": "0.2.2",
    "chai": "3.5.0",
    "git-issues": "1.3.1",
    "mocha": "4.1.0",
    "nock": "9.6.1",
    "semantic-release": "15.13.31",
    "sinon": "1.17.7",
    "sinon-as-promised": "4.0.3",
    "sinon-chai": "2.14.0",
    "snap-shot-it": "4.1.5"
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "break",
          "release": "major"
        }
      ]
    }
  }
}
